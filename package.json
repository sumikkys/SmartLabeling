{
  "name": "smartlabeling",
  "author": "SmartLabeling Team",
  "description": "About A data labeling tool with AI from Segment Anything and other models.",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "electron:build": "electron-builder"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "pinia": "^3.0.1",
    "tree-kill": "^1.2.2",
    "vue": "^3.5.13",
    "webworker": "^0.8.4"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.16",
    "@vitejs/plugin-vue": "^5.2.3",
    "electron": "^30.0.1",
    "electron-builder": "^26.0.12",
    "typescript": "^5.2.2",
    "vite": "^5.1.6",
    "vite-plugin-electron": "^0.28.6",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vue-tsc": "^2.0.26"
  },
  "main": "dist-electron/main.js",
  "repository": {
    "type": "git",
    "url": "(https://github.com/summikkys/SmartLabeling)"
  },
  "keywords": [
    "123456"
  ],
  "license": "MIT",
  "build": {
    "appId": "com.example.smartlabeling",
    "productName": "SmartLabeling",
    "compression": "maximum",
    "files": [
      "dist/**/*",
      "dist-electron/**/*",
      "core/**/*",
      "public/**/*"
    ],
    "directories": {
      "output": "release"
    },
    "asar" : true,
    "asarUnpack": [
      "core/**/*"
    ],
    "win": {
      "target": "nsis",
      "icon": "public/SmartLabeling.ico",
      "signAndEditExecutable": false
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "AppImage"
    },
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "public/SmartLabeling.ico",
      "perMachine": true,
      "deleteAppDataOnUninstall": true
    }
  }
}
